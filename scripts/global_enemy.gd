extends Node

var enemyName = "NAME"
var enemyLife = 100
var enemyAtaque = 0

func setEnemyName(name):
	enemyName = name
func getEnemyName():
	return enemyName

func setEnemyLife(life):
	enemyLife = life
func getEnemyLife():
	return enemyLife

func setEnemyAtaque(ataque):
	enemyAtaque = ataque
func getEnemyAtaque(ataque):
	return ataque